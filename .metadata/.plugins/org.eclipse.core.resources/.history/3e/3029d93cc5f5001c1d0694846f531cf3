package java0627;

import java.util.Scanner;

public class ex04누적합예제 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num =0;
		while(num>=0 && num<=100) {		
			System.out.println("숫자를 입력하세요 : ");
			num = sc.nextInt();			
			(num + num);
			System.out.println("합계 : " + (num);
			
		}
		
	
		
	}

}
