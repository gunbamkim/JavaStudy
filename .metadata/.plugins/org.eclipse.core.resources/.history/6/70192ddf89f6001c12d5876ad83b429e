package java0628;

import java.util.Scanner;

public class ex04누적합 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int num1 = sc.nextInt();
		int num2 = sc.nextInt();
		int sum = 0;
		
		int a = num1>num2?num2:num1;
		for(int i = a;i<=(num1+num2)-a;i++) {
			sum=(sum+i);
			System.out.println(sum);
		}
		

	}

}
