package Java0622;

public class ex03 {
		public static void main(String[] args) {
			int num1 = 7;
			int num2 = 3;
			
			System.out.println("더하기결과" + (num1 + num2));
			System.out.println(num1 - num2);
			System.out.println(num1 * num2);
			System.out.println((float)(num1 / num2) + (floay)(num1 % num2));
		
		}
}
