package Java0622;

public class 산술연산자 {
	// 프로그램의 시작점
	public static void main(String[] args) {
		int a = 100;
		int b = 200;
		
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b); // 몫
		System.out.println(a%b); // 나머지

		float num1 = 3.14f;
		float num2 = 16.999f;
		
		System.out.println(a + num1);
		System.out.println(a - num2);
		
		
	
		
	}
}
