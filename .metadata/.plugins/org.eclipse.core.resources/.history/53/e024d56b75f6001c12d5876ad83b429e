package java0621;

public class ex01data {
	//프로그램 시작점
	public static void main(String[] args) {
		// 변수선언
		// 논리
		boolean bool = true;
		boolean bool2 = false;
		// 정수
		byte byte1 = 128;
		short short2 = 32000;
		int int3 = 50000000;
		long long1 = 10000000000000L;
		// 실수
		float float1 = 5.16f;
		double double2 = 5.16;
		//정수(글자)
		char char1 = 'A';
	}
}
