package Java0622;

public class ex05시분초_변환기 {
		public static void main(String[] args) {
			// 정수형 변수에 초를 초기화
			int second = 662;
					
			// 초단위의 값을 시/분/초 형태로 변환하기
			// 결과값 : 0시 0분 0초
					
			//코드 작성
			
			int 1 = second /3600
			int 2 = second / 60	
			int 3 = second / 1
			System.out.println();
			
			System.out.println(second / 60);
			
		}
}
