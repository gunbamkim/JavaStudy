public class ee2222 {

	public static void main(String[] args) {
		
		
		for(int a = 1; a <=9; a++) {
			for(int c=1; c<=9; c++) {
			System.out.println(c + "ë‹¨");
			
			
			for( int b = 2; b<=9; b++) {
			
			
				System.out.print(b + "*" + a + "=" + a*b+" ");
			}
			}
			}
	}

}
