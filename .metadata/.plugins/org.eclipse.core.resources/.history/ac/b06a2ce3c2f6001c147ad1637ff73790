package java0628;

public class ee11 {

	public static void main(String[] args) {
		
		int a=1;
			
		for(int j=2;j<=9;j++) {
			System.out.println(j+"ë‹¨ : ");
			int b =-1;
			b+=1;
			for(int i = 2; i<=9;i++) {
				a+=1;
				
				b+=1;
				System.out.print(i+"*"+b +"="+i*b+" ");
				}
		}

	}

}

